TARGET= COE597_Project

###############################################################################
# Building variables
###############################################################################
# debug build?
DEBUG=0
# optimization
OPT = -Og

###############################################################################
# paths
###############################################################################
# Build directory
BLD_DIR = build

###############################################################################
# source
###############################################################################
# C source files
SRC_C = $(wildcard *.c)

$(BLD_DIR):
	mkdir -p $(BLD_DIR)

clean:
	rm -rf $(BLD_DIR)

-include $(wildecard $(BLD_DIR)/*.d)

